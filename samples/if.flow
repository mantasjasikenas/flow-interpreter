fun set(num: Int): Int {
    if (num < 0) {
        return -1;
    } else {
        if (num > 0) {
            if (num > 10) {
                return 2;
            } else {
                return 1;
                println("This line will not be printed.");
            }
        } else {
            return 0;
        }
    }

    return 0;
}

var a = set(6);
println("Returned value:" + a);

a = set(0);
println("Returned value:" + a);