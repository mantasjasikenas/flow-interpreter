class Printer {
    // allow to assign value to val in constructor
    val header: String;

    constructor(text: String) {
        header = text;
    }

    fun printConsole(text: String): Unit {
        println("Object " + header + " " + text);
    }
}


class Calculator {

    fun fibonacci(n: Int): Int {
        if (n <= 1) {
            return n;
        }

        return fibonacci(n - 1) + fibonacci(n - 2);
    }
}

val value1 = 10;
val value2 = 20;

val calculator = new Calculator();

val result1 = calculator.fibonacci(value1);
val result2 = calculator.fibonacci(value2);

val header1 = "Fibonacci_1";
val header2 = "Fibonacci_2";

val printer1 = new Printer (header1);
var printer2 = new Printer (header2);

printer1 = printer2;
printer1.printConsole("same object");
printer2.printConsole("same object");


